<Project Sdk="Microsoft.NET.Sdk">
  <PropertyGroup>
    <TargetFrameworks>netstandard2.0;net45</TargetFrameworks>
    <RootNamespace>AntiCaptcha</RootNamespace>
    <AssemblyName>CSharp.AntiCaptcha</AssemblyName>
    <GeneratePackageOnBuild>true</GeneratePackageOnBuild>
    <IncludeSource>True</IncludeSource>
    <IncludeSymbols>True</IncludeSymbols>
	<RunPostBuildEvent>OnOutputUpdated</RunPostBuildEvent>
	<Version>1.0.0</Version>
	<RepositoryUrl>https://github.com/chancity/CSharp.AntiCaptcha</RepositoryUrl>
	<PackageProjectUrl>https://github.com/chancity/CSharp.AntiCaptcha</PackageProjectUrl>
  </PropertyGroup>
  <ItemGroup>
    <PackageReference Include="Newtonsoft.Json" Version="13.0.2" />
    <PackageReference Include="System.Net.Http" Version="4.3.4" />
  </ItemGroup>
</Project>